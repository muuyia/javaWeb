<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">

    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>com.cainiao.servlet.Aa_Helloworld</servlet-class>

    </servlet>
<!--http://localhost:8080/cainiao_Servlet/HelloWorldUrl  success  -->
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorldUrl</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>HelloForm</servlet-name>
        <servlet-class>com.cainiao.servlet.Ab_HelloForm_Get</servlet-class>
    </servlet>
<!--http://localhost:8080/cainiao_Servlet/HelloFormUrl?name=菜鸟教程&url=www.runoob.com  success -->
    <servlet-mapping>
        <servlet-name>HelloForm</servlet-name>
        <url-pattern>/HelloFormUrl</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HelloFormPost</servlet-name>
        <servlet-class>com.cainiao.servlet.Ab_HelloForm_Post</servlet-class>
    </servlet>
    <!--http://localhost:8080/cainiao_Servlet/HelloFormUrl?name=菜鸟教程&url=www.runoob.com  success -->
    <servlet-mapping>
        <servlet-name>HelloFormPost</servlet-name>
        <url-pattern>/HelloFormPostUrl</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckBox</servlet-name>
        <servlet-class>com.cainiao.servlet.Abc_CheckBox</servlet-class>
    </servlet>
    <!-- -->
    <servlet-mapping>
        <servlet-name>CheckBox</servlet-name>
        <url-pattern>/CheckBox</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadParams</servlet-name>
        <servlet-class>com.cainiao.servlet.Abd_ReadParams</servlet-class>
    </servlet>
    <!-- -->
    <servlet-mapping>
        <servlet-name>ReadParams</servlet-name>
        <url-pattern>/ReadParams</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PageRedirect</servlet-name>
        <servlet-class>com.cainiao.servlet.PageRedirect</servlet-class>
    </servlet>
    <!-- http://localhost:8080/cainiao_Servlet/PageRedirect success-->
    <servlet-mapping>
        <servlet-name>PageRedirect</servlet-name>
        <url-pattern>/PageRedirect</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DisplayHeader</servlet-name>
        <servlet-class>com.cainiao.servlet.DisplayHeader</servlet-class>
    </servlet>
    <!-- http://localhost:8080/cainiao_Servlet/DisplayHeader   success-->
    <servlet-mapping>
        <servlet-name>DisplayHeader</servlet-name>
        <url-pattern>/DisplayHeader</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Refresh</servlet-name>
        <servlet-class>com.cainiao.servlet.Refresh</servlet-class>
    </servlet>
    <!-- http://localhost:8080/cainiao_Servlet/Refresh   success-->
    <servlet-mapping>
        <servlet-name>Refresh</servlet-name>
        <url-pattern>/Refresh</url-pattern>
    </servlet-mapping>


</web-app>